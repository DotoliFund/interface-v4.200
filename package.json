  "name": "myApp",
  "version": "0.1.0",
  "description": "myApp Interface",
  "homepage": ".",
  "license": "GPL-3.0-or-later",
  "private": true,
  "scripts": {
    "contracts:compile:abi": "typechain --target ethers-v5 --out-dir src/abis/types \"./src/abis/**/*.json\"",
    "contracts:compile:v3": "typechain --target ethers-v5 --out-dir src/types/v3 \"./node_modules/@uniswap/**/artifacts/contracts/**/*[!dbg].json\"",
    "contracts:compile:fund": "typechain --target ethers-v5 --out-dir src/types/fund \"../myContract/artifacts/contracts/**/*[!dbg].json\"",
    "contracts:compile": "yarn contracts:compile:abi && yarn contracts:compile:v3 && yarn contracts:compile:fund",
    "prei18n:extract": "node prei18n-extract.js",
    "i18n:extract": "lingui extract --locale en-US",
    "i18n:compile": "yarn i18n:extract && lingui compile",
    "i18n:pseudo": "lingui extract --locale pseudo && lingui compile",
    "prepare": "yarn contracts:compile && yarn i18n:compile",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test --coverage",
    "lint": "yarn eslint .",
    "cypress:open": "cypress open --browser chrome --e2e",
    "cypress:run": "cypress run --browser chrome --e2e"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.ts*",
      "src/hooks/**/*.ts*",
      "src/lib/hooks/**/*.ts*",
      "src/lib/state/**/*.ts*",
      "src/lib/utils/**/*.ts*",
      "src/pages/**/*.ts*",
      "src/state/**/*.ts*",
      "src/utils/**/*.ts*"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 4,
        "functions": 6,
        "lines": 9,
        "statements": 9
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
